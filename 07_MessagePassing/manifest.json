{
    "manifest_version": 2,
    "name": "Link Count -> Browser Action",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["event.js"],
        "persistent": false
    }
}